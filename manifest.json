{
  "name": "Authentication Needed",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "blocked_domains.js",
      "background.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}